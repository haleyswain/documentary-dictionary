<h1>New Movie</h1>

<%= form_for @movie do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :director %>
  <%= f.text_field :director %>
  <%= f.select :genre, options_for_select([['Select Genre', ""], 'Nature', 'Food and Health', 'Travel', 'Drama', 'History', 'Other' ]) %>
  <%= f.submit %>
<% end %>
